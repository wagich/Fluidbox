{
	"compilerOptions": {
		"strict": true,
		"sourceMap": true,
		"target": "es5",
        "module": "es6",
		"declaration": true,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"strictPropertyInitialization": false,
		"moduleResolution": "node",
		"downlevelIteration": true,
		"lib": [ "dom", "dom.iterable", "es2015", "es5", "scripthost" ],
		"outDir": "./dist/js"
	},
	"exclude": [
		"node_modules"
	]
}
